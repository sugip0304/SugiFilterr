platform :ios, '9.0'

target 'SugiFilter' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SugiFilter
  pod 'RxSwift',    '~> 3.0'
  pod 'RxCocoa',    '~> 3.0'
  pod 'Moya/RxSwift', '~> 8.0.5'
  pod 'Moya-ObjectMapper/RxSwift', '~> 2.3.2'
  pod 'SDWebImage', '~> 4.0'
  pod 'SwiftRangeSlider'

  target 'SugiFilterTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 3.0'
    pod 'RxTest',     '~> 3.0'
  end

end
